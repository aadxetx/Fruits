    {
        "body": {
            "magepowAjaxcart": {
                "countDown": "1",
                "showLoader": "1",
                "addUrl": "https://freshbasket.com.pk/khi/ajaxcart/",
                "quickview": false,
                "quickViewUrl": "https://freshbasket.com.pk/khi/ajaxcart/index/view/",
                "isProductView": false,
